

#include <iostream>

using namespace std;

int main()
{
  
    int a[]={0,1,2,5,9};
    int b[]={1,2,3,4,7};
    int n=5;
    int j=0;
    int i=0;
   while((i <n)  && (j <n))
    {
        if(a[i]<b[j])
        {
            i++;
        }
        if(a[i]==b[j])
        {
            cout<<a[i];
            i++;
            j++;
        }
        else
        {
            j++;
        }
        
        
    }


  
    return 0;
}
