

#include <iostream>

using namespace std;

int main()
{
  int a[]={0,1,2,5,9};
  int n=sizeof(a)/sizeof(a[0]);
  for(int i=0;i<n;i++)
  {
      for (int j=i;j<n;j++)
      {
          for(int k=i;k<=j;k++)
          {
              cout<<a[k]<<" ";
          }
          cout<<endl;
      }
  }
 return 0;
}
