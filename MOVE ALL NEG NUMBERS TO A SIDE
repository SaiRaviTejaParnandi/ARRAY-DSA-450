#include <iostream>

using namespace std;

int main()
{
  
    int a[]={-5,0,21,0,4,-1};
    int n= (sizeof(a)) / sizeof(a[0]);
    int j=0;
    for(int i=0;i<n;i++)
    {
        if(a[i]<0)
        {
            swap(a[j],a[i]);
            j++;
        }
        
    }

     for(int i=0;i<n;i++)
    {
        cout<<a[i]<<endl;
    }
    return 0;
}
