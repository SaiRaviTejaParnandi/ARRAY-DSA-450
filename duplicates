    int findDuplicate(vector<int>& nums) {
        int k=nums.size();
        int a[k],x;
        for (int i=0;i<k;i++)
        {   
            a[i]=0;
        }
        for (int i=0;i<k;i++)
        {   
            if (a[nums[i]]==0)
               a[nums[i]]=1;
            else
               x=nums[i];
        }
        return x;
    }
};
