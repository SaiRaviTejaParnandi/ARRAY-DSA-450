#include <iostream>

using namespace std;

int main()
{
  
    int a[]={0,1,2,5,9};
    int b[]={1,2,3,4,7};
    int n=5;
    int j=0;
    int i=0;
   while((i <n)  && (j <n))
    {
        if(a[i]<b[j])
        {
            cout<<a[i];
            i++;
        }
        if(a[i]==b[j])
        {
            cout<<a[i];
            i++;
            j++;
        }
        else
        {
            cout<<b[j];
            j++;
        }
        
        
    }
    while(i<n)
    {
        cout << a[i++];
    }
    while(j<n)
    {
        cout<<b[j++];
    }

  
    return 0;
}
